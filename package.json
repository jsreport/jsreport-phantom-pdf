{
  "name": "jsreport-phantom-pdf",
  "version": "1.3.4-beta",
  "description": "jsreport recipe which is rendering pdf from html using phantomjs",
  "scripts": {
    "test": "mocha test --timeout 7000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-phantom-pdf",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-phantom-pdf.git"
  },
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.4.7",
    "in-publish": "2.0.0",
    "node.extend": "1.1.6",
    "phantom-html-to-pdf": "0.4.12",
    "stream-to-array": "2.3.0"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "eslint": "3.14.1",
    "eslint-config-standard-jsx": "3.2.0",
    "eslint-plugin-babel": "4.0.1",
    "jsreport-core": "1.1.0",
    "jsreport-debug": "1.0.3",
    "jsreport-studio-dev": "1.0.2",
    "jsreport-templates": "1.1.0",
    "mocha": "3.2.0",
    "phantomjs-exact-2-1-1": "0.1.0",
    "should": "11.2.0",
    "standard": "8.6.0"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
